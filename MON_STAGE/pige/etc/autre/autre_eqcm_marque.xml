  <navigation>
    <profondeur>1000</profondeur>
    <pause>0</pause>
    <timeout>60</timeout>
    <thread>10</thread>
    <rang>
      <a_suivre>
        <expression>&lt;a href=&quot;([^&quot;]+)&quot; hreflang=&quot;fr&quot; title=&quot;Suivant&quot; class=&quot;LinkIn&quot;&gt;&amp;gt;&lt;\/a&gt;</expression>
        <remplacement>substr($entete['location'],0,strrpos($entete['location'],'/')+1).'$1'</remplacement>
        <option>e</option>
        <limite>1</limite>
        <suivant>0</suivant>
      </a_suivre>
      <a_capturer>
        <expression>&lt;div class=&quot;resume-texte&quot;&gt;[^&lt;]+&lt;a[^&gt;]+&gt;[^&lt;]+(&lt;span class=&quot;resume-marque&quot;&gt;[^&lt;]+&lt;\/span&gt;[^&lt;]+&lt;span class=&quot;resume-separateur&quot;&gt; \| &lt;\/span&gt;[^&lt;]+&lt;span class=&quot;resume-reference&quot;&gt;[^&lt;]+&lt;\/span&gt;)</expression>
        <remplacement>$1</remplacement>
        <option>s</option>
        <limite>1000</limite>
      </a_capturer>
    </rang>
  </navigation>
