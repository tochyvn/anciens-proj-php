<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="kelkoo_marques">
  <initialisation>
    <url>:parametre1:</url>
    <parametres>
      <parametre>
        <valeur>http://mesnotices.fr/letter_a.htm</valeur>
        <valeur>http://mesnotices.fr/letter_b.htm</valeur>
        <valeur>http://mesnotices.fr/letter_c.htm</valeur>
        <valeur>http://mesnotices.fr/letter_d.htm</valeur>
        <valeur>http://mesnotices.fr/letter_e.htm</valeur>
        <valeur>http://mesnotices.fr/letter_f.htm</valeur>
        <valeur>http://mesnotices.fr/letter_g.htm</valeur>
        <valeur>http://mesnotices.fr/letter_h.htm</valeur>
        <valeur>http://mesnotices.fr/letter_i.htm</valeur>
        <valeur>http://mesnotices.fr/letter_j.htm</valeur>
        <valeur>http://mesnotices.fr/letter_k.htm</valeur>
        <valeur>http://mesnotices.fr/letter_l.htm</valeur>
        <valeur>http://mesnotices.fr/letter_m.htm</valeur>
        <valeur>http://mesnotices.fr/letter_n.htm</valeur>
        <valeur>http://mesnotices.fr/letter_o.htm</valeur>
        <valeur>http://mesnotices.fr/letter_p.htm</valeur>
        <valeur>http://mesnotices.fr/letter_q.htm</valeur>
        <valeur>http://mesnotices.fr/letter_r.htm</valeur>
        <valeur>http://mesnotices.fr/letter_s.htm</valeur>
        <valeur>http://mesnotices.fr/letter_t.htm</valeur>
        <valeur>http://mesnotices.fr/letter_u.htm</valeur>
        <valeur>http://mesnotices.fr/letter_v.htm</valeur>
        <valeur>http://mesnotices.fr/letter_w.htm</valeur>
        <valeur>http://mesnotices.fr/letter_x.htm</valeur>
        <valeur>http://mesnotices.fr/letter_y.htm</valeur>
        <valeur>http://mesnotices.fr/letter_z.htm</valeur>
        <valeur>http://mesnotices.fr/letter_others.htm</valeur>
      </parametre>
    </parametres>
  </initialisation>
  <navigation>
    <profondeur>1000</profondeur>
    <pause>0</pause>
    <timeout>60</timeout>
    <thread>10</thread>
    <rang>
      <a_suivre>
        <expression>href=&quot;(\/marque-manuel-notice-mode-emploi\/[^&quot;]+)&quot;</expression>
        <remplacement>http://mesnotices.fr$1</remplacement>
        <limite>10000</limite>
        <suivant>1</suivant>
      </a_suivre>
    </rang>
    <rang>
      <a_suivre>
        <expression>href=&quot;(\/reference-[^\/]+\/1)&quot;</expression>
        <remplacement>http://mesnotices.fr$1</remplacement>
        <limite>1</limite>
        <suivant>2</suivant>
      </a_suivre>
    </rang>
    <rang>
      <a_suivre>
        <expression>&lt;b&gt; [0-9]+ &lt;\/b&gt;&lt;a href=&quot;(\/reference-[^\/]+\/[0-9]+)&quot;&gt;[0-9]+&lt;\/a&gt;</expression>
        <remplacement>http://mesnotices.fr$1</remplacement>
        <limite>1</limite>
        <suivant>2</suivant>
      </a_suivre>
      <a_capturer>
        <expression>href=&quot;(\/manuel-notice-mode-emploi\/[^\/]+\/[^&quot;]+)&quot;</expression>
        <remplacement>http://mesnotices.f$1</remplacement>
        <option>s</option>
        <limite>1000</limite>
      </a_capturer>
    </rang>
  </navigation>
  <capture>
    <destination>autre_mesnotices_marque.csv</destination>
    <echappement>\</echappement>
    <separateur>;</separateur>
    <encapsulateur>"</encapsulateur>
    <ligne>CRLF</ligne>
    <enlever_balise>1</enlever_balise>
    <enlever_entite>1</enlever_entite>
    <parent>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <option>s</option>
    </parent>
    <enfant>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <limite>1</limite>
      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="expression" nom="marque">
        <expression>\/([^\/]+)\/([^\/]+)-_F$</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="reference">
        <expression>\/([^\/]+)\/([^\/]+)-_F$</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="location">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
    </colonnes>
  </capture>
</spool>
