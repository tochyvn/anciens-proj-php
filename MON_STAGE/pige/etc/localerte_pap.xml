<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_pap">
  <initialisation>
    <url>http://www.pap.fr/annonce/locations-:parametre1:</url>
    <parametres>
      <parametre>
        <valeur>maison-studio</valeur>
        <valeur>maison-2-pieces</valeur>
        <valeur>maison-3-pieces</valeur>
        <valeur>maison-4-pieces</valeur>
        <valeur>maison-5-pieces</valeur>
        <valeur>maison-6-pieces</valeur>
        <valeur>maison-7-pieces</valeur>
        <valeur>appartement-studio</valeur>
        <valeur>appartement-2-pieces</valeur>
        <valeur>appartement-3-pieces</valeur>
        <valeur>appartement-4-pieces</valeur>
        <valeur>appartement-5-pieces</valeur>
        <valeur>appartement-6-pieces</valeur>
        <valeur>appartement-7-pieces</valeur>
        <valeur>loft</valeur>
        <valeur>parking</valeur>
        <valeur>peniche</valeur>
        <valeur>-mobil-home</valeur>
        <valeur>location-accession</valeur>
        <valeur>residence-avec-service</valeur>
        <valeur>logement-contre-services</valeur>
        <valeur>local-d-activite</valeur>
        <valeur>divers</valeur>
      </parametre>
    </parametres>
  </initialisation>
  <navigation>
    <profondeur>700</profondeur>
    <pause>0</pause>
    <timeout>120</timeout>
    <thread>500</thread>
    <user_agent>Mozilla/5.0 (compatible; Yahoo! Slurp; http://help.yahoo.com/help/us/ysearch/slurp)</user_agent>
    <!-- user_agent>Mozilla/2.0 (compatible; Ask Jeeves/Teoma)</user_agent -->
    <!-- user_agent>Mozilla/5.0 (compatible; Googlebot/1.0; +http://www.google.com/bot.html)</user_agent -->
    <!-- user_agent>Mozilla/5.0 (compatible; Aicombot/1.0; +http://www.aicom-multimedia.com)</user_agent -->
    <rang>
      <a_suivre>
        <expression>&lt;a href=&quot;([^&quot;]+)&quot;&gt;Suivante&lt;\/a&gt;</expression>
        <remplacement>http://www.pap.fr$1</remplacement>
        <limite>1</limite>
        <suivant>0</suivant>
      </a_suivre>
      <a_capturer>
        <expression>(&lt;li class=&quot;annonce&quot;(.*\n){40})</expression>
        <remplacement>$1</remplacement>
        <limite>100</limite>
      </a_capturer>
    </rang>
  </navigation>
  <capture>
    <destination>localerte_pap.csv</destination>
    <echappement>\</echappement>
    <separateur>;</separateur>
    <encapsulateur>"</encapsulateur>
    <ligne>CRLF</ligne>
    <enlever_balise>1</enlever_balise>
    <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <option>s</option>
    </parent>
    <enfant>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <limite>1</limite>
      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Nom">
        <valeur>DeParticulierAParticulier</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>&lt;p class=&quot;date-publication&quot;&gt;[^&lt;]+([0-9]{2} [^ ]+ [0-9]{4})&lt;\/p&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <option>i</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^([0-9][^0-9])</expression>
            <remplacement>0$1</remplacement>
          </filtre>
          <filtre>
            <expression>\/([0-9][^0-9])</expression>
            <remplacement>/0$1</remplacement>
          </filtre>
          <filtre>
            <expression>\/([0-9]{2})$</expression>
            <remplacement>/20$1</remplacement>
          </filtre>
          <filtre>
            <expression>^([0-9]+) janvier ([0-9]+)$</expression>
            <remplacement>$1/01/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) f..?vrier ([0-9]+)$</expression>
            <remplacement>$1/02/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mars ([0-9]+)$</expression>
            <remplacement>$1/03/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) avril ([0-9]+)$</expression>
            <remplacement>$1/04/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mai ([0-9]+)$</expression>
            <remplacement>$1/05/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) juin ([0-9]+)$</expression>
            <remplacement>$1/06/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) juillet ([0-9]+)$</expression>
            <remplacement>$1/07/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) ao..?t ([0-9]+)$</expression>
            <remplacement>$1/08/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) septembre ([0-9]+)$</expression>
            <remplacement>$1/09/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) octobre ([0-9]+)$</expression>
            <remplacement>$1/10/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) novembre ([0-9]+)$</expression>
            <remplacement>$1/11/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) d..?cembre ([0-9]+)$</expression>
            <remplacement>$1/12/$2</remplacement>
            <option>i</option>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression>&lt;div id=&quot;annonce_resume_([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>&lt;b&gt;([^\.]+)\.&lt;\/b&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>(.+) \([0-9]+\)$</expression>
            <remplacement>$1</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="CP">
        <expression>&lt;b&gt;([^\.]+)\.&lt;\/b&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[^0-9]+</expression>
            <remplacement></remplacement>
          </filtre>
          <filtre>
            <expression>^([0-9]{1,4}|[0-9]{6,})$</expression>
            <remplacement></remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>&lt;td class=&quot;lien-annonce&quot;&gt;&lt;a href=&quot;[^&quot;]+&quot;&gt;Location ([^&lt;]+)&lt;\/a&gt;&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^.*(studio|maison|garage, parking|colocation).*</expression>
            <remplacement>$1</remplacement>
          </filtre>
          <filtre>
            <expression>^.*appartement ([0-9]+).+</expression>
            <remplacement>T$1</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>&lt;td class=&quot;prix&quot;&gt;(.+)&amp;nbsp;&amp;euro;&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[^0-9]+</expression>
            <remplacement></remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Descriptif">
        <expression>&lt;p class=&quot;annonce-resume-texte&quot;&gt;([^&lt;]+&lt;b&gt;[^&lt;]+&lt;\/b&gt;.+)\n					&lt;\/[p]&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <option>s</option>
        <filtres>
          <filtre>
            <expression>ou envoyer un e-mail au propriétaire</expression>
            <remplacement></remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;td class=&quot;lien-annonce&quot;&gt;&lt;a href=&quot;[^&quot;]+&quot;&gt;([^&gt;]+)&lt;\/a&gt;&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Part/Pro">
        <valeur>PARTICULIER</valeur>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;img[^&gt;]+src=&quot;(http:\/\/static[^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>&lt;li class=&quot;detail&quot;&gt;&lt;a href=&quot;([^&quot;]+)&quot; rel=&quot;nofollow&quot;&gt;D</expression>
        <remplacement>http://www.pap.fr$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Email">
        <valeur />
      </colonne>
      <colonne mode="valeur" nom="Tél">
        <valeur />
      </colonne>
    </colonnes>
  </capture>
</spool>
