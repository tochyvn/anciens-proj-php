<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_ebay">
  <initialisation>
    <url>http://immobilier.shop.ebay.fr/Location-/147426/i.html?LH_CAds=1&amp;LH_TitleDesc=0&amp;LH_PrefLoc=0&amp;cmd=Blend&amp;_dmd=2&amp;cmd=Blend&amp;_trksid=p1703029.c0.m282&amp;_catref=1&amp;_fln=1</url>
  </initialisation>
  <navigation>
     <profondeur>40</profondeur>
     <pause>0</pause>
     <timeout>60</timeout>
     <thread>5</thread>
    <rang>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot; pagn=&quot;next&quot;</expression>
         <remplacement>$1</remplacement>
         <limite>1</limite>
         <suivant>0</suivant>
       </a_suivre>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot; class=&quot;img&quot; itemprop=&quot;url&quot;&gt;</expression>
         <remplacement>$1</remplacement>
         <limite>100</limite>
         <suivant>1</suivant>
       </a_suivre>
    </rang>
    <rang>
      <a_capturer>
         <expression>(.+)</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>1</limite>
       </a_capturer>
    </rang>
  </navigation>
  <capture>
     <destination>localerte_ebay.csv</destination>
     <echappement>\</echappement>
     <separateur>;</separateur>
     <encapsulateur>"</encapsulateur>
     <ligne>CRLF</ligne>
     <enlever_balise>1</enlever_balise>
     <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <option>s</option>
     </parent>
    <enfant>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <limite>1</limite>
	   <option>s</option>
     </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>eBay</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>Date de publication[^&lt;]+&lt;\/i&gt;&lt;span&gt;([0-9]+ [^0-9]+ [0-9]{2,4})</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
	    <option>i</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^([0-9][^0-9])</expression>
            <remplacement>0$1</remplacement>
          </filtre>
          <filtre>
            <expression>\/([0-9][^0-9])\/</expression>
            <remplacement>/0$1/</remplacement>
          </filtre>
          <filtre>
            <expression>\/([0-9]{2})$</expression>
            <remplacement>/20$1</remplacement>
          </filtre>
          <filtre>
            <expression>^([0-9]+) janv(ier|\.)? ([0-9]+)$</expression>
            <remplacement>$1/01/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) f..?vr(ier|\.)? ([0-9]+)$</expression>
            <remplacement>$1/02/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mars ([0-9]+)$</expression>
            <remplacement>$1/03/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) avr(il|\.)? ([0-9]+)$</expression>
            <remplacement>$1/04/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mai ([0-9]+)$</expression>
            <remplacement>$1/05/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) jui(n|\.)? ([0-9]+)$</expression>
            <remplacement>$1/06/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) juil(let|\.)? ([0-9]+)$</expression>
            <remplacement>$1/07/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) ao..?t ([0-9]+)$</expression>
            <remplacement>$1/08/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) sept(embre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/09/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) oct(obre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/10/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) nov(embre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/11/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) d..?c(embre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/12/$3</remplacement>
            <option>i</option>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression>\/([0-9]+)\?</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>se trouve l'objet[^&lt;]+&lt;\/i&gt;&lt;span&gt;([0-9]{5}), ([^,]+),</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Code postal">
        <expression>se trouve l'objet[^&lt;]+&lt;\/i&gt;&lt;span&gt;([0-9]{5}), ([^,]+),</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>Type: &lt;\/th&gt;&lt;td[^&gt;]+&gt;([^&lt;]+)&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>&amp;price=([^&amp;]+)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>&lt;div class=&quot;item_description&quot;&gt;&lt;div id=&quot;ngvi_desc_div&quot; class=&quot;d-pad&quot;&gt;&lt;div&gt;&lt;div&gt;(.+)&lt;\/div&gt;&lt;\/div&gt;&lt;\/div&gt;&lt;\/div&gt;&lt;center&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;h1[^&gt;]*&gt;([^&lt;]+)&lt;\/h1&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Part/Pro">
        <expression>&lt;span class=&quot;vi-cf-ca&quot;&gt; ([^&lt;]+)&lt;\/span&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche></recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;center&gt;&lt;span&gt;&lt;\/span&gt;&lt;img src=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Adresse email">
        <expression></expression>
        <remplacement></remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Tél">
        <expression>&gt;(0[0-9]{9})&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
    </colonnes>
  </capture>
</spool>
