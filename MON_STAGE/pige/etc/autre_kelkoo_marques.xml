<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="kelkoo_marques">
  <initialisation>
    <url>http://www.kelkoo.fr/bz-:parametre1:-brands</url>
    <parametres>
      <parametre>
        <valeur>a</valeur>
        <valeur>b</valeur>
        <valeur>c</valeur>
        <valeur>d</valeur>
        <valeur>e</valeur>
        <valeur>f</valeur>
        <valeur>g</valeur>
        <valeur>h</valeur>
        <valeur>i</valeur>
        <valeur>j</valeur>
        <valeur>k</valeur>
        <valeur>l</valeur>
        <valeur>m</valeur>
        <valeur>n</valeur>
        <valeur>o</valeur>
        <valeur>p</valeur>
        <valeur>q</valeur>
        <valeur>r</valeur>
        <valeur>s</valeur>
        <valeur>t</valeur>
        <valeur>u</valeur>
        <valeur>v</valeur>
        <valeur>w</valeur>
        <valeur>x</valeur>
        <valeur>y</valeur>
        <valeur>z</valeur>
        <valeur>0</valeur>
      </parametre>
    </parametres>
  </initialisation>
  <navigation>
    <profondeur>100</profondeur>
    <pause>0</pause>
    <timeout>60</timeout>
    <thread>5</thread>
    <rang>
      <a_suivre>
        <expression>&lt;li&gt;&lt;a href=&quot;([^&quot;]+)&quot;&gt;Suivant&lt;\/a&gt;&lt;\/li&gt;</expression>
        <remplacement>$1</remplacement>
        <limite>1</limite>
        <suivant>0</suivant>
      </a_suivre>
      <a_capturer>
        <expression>&lt;img src=&quot;http:\/\/r6\.kelkoo\.com\/ux\/(placeholders|brands\/small)\/[^&quot;]+&quot; width=&quot;50&quot; height=&quot;50&quot; alt=&quot;&quot;&gt;[^a-zA-Z0-9&lt;]+([^&lt;]+)</expression>
        <remplacement>$2</remplacement>
        <option>s</option>
        <limite>100</limite>
      </a_capturer>
    </rang>
  </navigation>
  <capture>
    <destination>kelkoo_marque.csv</destination>
    <echappement>\</echappement>
    <separateur>;</separateur>
    <encapsulateur>"</encapsulateur>
    <ligne>CRLF</ligne>
    <enlever_balise>1</enlever_balise>
    <enlever_entite>1</enlever_entite>
    <parent>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <option>s</option>
    </parent>
    <enfant>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <limite>1</limite>
      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="expression" nom="">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
		<!--filtres>
		  <filtre>
		    <remplacement>-</remplacement>
        	<recherche> </recherche>
		  </filtre>
		</filtres-->
      </colonne>
    </colonnes>
  </capture>
</spool>
