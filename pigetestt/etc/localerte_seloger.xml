    </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>SeLoger</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>.</expression>
        <remplacement>date('d/m/Y')</remplacement>
        <recherche>ENFANT</recherche>
        <option>e</option>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression>\/([0-9]+)\.htm</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>&lt;input type=&quot;hidden&quot; name=&quot;ville&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Code postal">
        <expression>&lt;input type=&quot;hidden&quot; name=&quot;codepostal&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>&lt;h1 class=&quot;detail-title&quot;&gt;(.+)&lt;\/h1&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^Appartement ([0-9]+) pi.+ces?.+</expression>
            <remplacement>T$1</remplacement>
          </filtre>
          <filtre>
            <expression>^Maison.+</expression>
            <remplacement>Maison</remplacement>
          </filtre>
          <filtre>
            <expression>^Studio.+</expression>
            <remplacement>Studio</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>resume__prix&quot;.+&gt;([0-9]+)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>,</expression>
            <remplacement>.</remplacement>
          </filtre>
          <filtre>
            <expression>[^0-9\.]+</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>&lt;input type=&quot;hidden&quot; name=&quot;description&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;h1 class=&quot;detail-title&quot;&gt;(.+)&lt;\/h1&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Part/Pro">
        <valeur>PROFESSIONNEL</valeur>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
	    <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;input type=&quot;hidden&quot; name=&quot;urlphoto&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Adresse email">
        <valeur></valeur>
      </colonne>
      <colonne mode="expression" nom="Tél">
        <expression>&lt;span class=&quot;hide&quot;&gt;([^&lt;]+)&lt;\/span&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>([^0-9]+)</expression>
            <remplacement></remplacement>
          </filtre>
          <filtre>
            <expression>^(0800|00800|0804|0805|0809|0810|0811|0819|0860|0820|0821|083664|089064|0825|0826|0884|0890|089071).+</expression>
            <remplacement></remplacement>
          </filtre>
        </filtres>
      </colonne>
