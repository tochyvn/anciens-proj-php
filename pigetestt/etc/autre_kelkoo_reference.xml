  <navigation>
    <profondeur>1000</profondeur>
    <pause>0</pause>
    <timeout>60</timeout>
    <thread>5</thread>
    <rang>
      <a_suivre>
        <expression>&lt;a href=&quot;([^&quot;]+)&quot;&gt;Suivant&lt;\/a&gt;</expression>
        <remplacement>$1</remplacement>
        <limite>1</limite>
        <suivant>0</suivant>
      </a_suivre>
      <a_capturer>
        <expression>&lt;a href=&quot;[^&quot;]+&quot; class=&quot;fn summary&quot;&gt;([^&lt;]+)&lt;\/a&gt;</expression>
        <remplacement>$1</remplacement>
        <option>s</option>
        <limite>1000</limite>
      </a_capturer>
    </rang>
  </navigation>
  <capture>
    <destination>autre_kelkoo_reference.csv</destination>
    <echappement>\</echappement>
    <separateur>;</separateur>
    <encapsulateur>"</encapsulateur>
    <ligne>CRLF</ligne>
    <enlever_balise>1</enlever_balise>
    <enlever_entite>1</enlever_entite>
    <parent>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <option>s</option>
    </parent>
    <enfant>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <limite>1</limite>
      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="expression" nom="marque">
        <expression>(.+)  (.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="reference">
        <expression>(.+)  (.+)</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="location">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
    </colonnes>
  </capture>
