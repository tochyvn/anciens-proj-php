<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_trefle">
  <initialisation>
    <url>http://www.trefle.com/immobilier/annonces-location-vides-loc-france.html?&amp;itemsByPage=&amp;order=&amp;itemsByPage=100</url>
  </initialisation>
  <navigation>
     <profondeur>600</profondeur>
     <pause>0</pause>
     <timeout>60</timeout>
     <thread>5</thread>
    <rang>
      <a_suivre>
         <expression>href=&quot;([^&quot;]+)&quot; class=&quot;suivant&quot;&gt;Suivante</expression>
         <remplacement>$1</remplacement>
         <limite>1</limite>
         <suivant>0</suivant>
       </a_suivre>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot; class=&quot;detail-product&quot;</expression>
         <remplacement>$1</remplacement>
         <limite>100</limite>
         <suivant>1</suivant>
       </a_suivre>
    </rang>
    <rang>
      <a_capturer>
         <expression>(.+)</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>1</limite>
       </a_capturer>
    </rang>
  </navigation>
  <capture>
     <destination>localerte_trefle.csv</destination>
     <echappement>\</echappement>
     <separateur>;</separateur>
     <encapsulateur>"</encapsulateur>
     <ligne>CRLF</ligne>
     <enlever_balise>1</enlever_balise>
     <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <option>s</option>
     </parent>
    <enfant>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <limite>1</limite>
	   <option>s</option>
     </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>Trefle</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>le : &lt;span class=&quot; bold noir&quot;&gt;([0-9]{2}\/[0-9]{2}\/[0-9]{2})</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression> : &lt;span class=&quot; bold noir&quot;&gt;([^&lt;]+)&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>margin-bottom:8px;&quot;&gt;([^(]+) \(</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Code Postal">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>Nombre de pi&amp;egrave;ce : &lt;b&gt;([0-9])</expression>
        <remplacement>T$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>span class=&quot;prix&quot;&gt;([0-9,]+)&amp;nbsp;&amp;euro;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>\t{4}&lt;div&gt;([^&lt;]+)&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
	    <option>s</option>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;span class=&quot;span&quot;&gt;([^&lt;]+)&lt;\/span&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Part/Pro">
        <valeur>PARTICULIER</valeur>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>id=&quot;image&quot; onclick=&quot;[^&quot;]+&quot; class=&quot;[^&quot;]+&quot; style=&quot;background\-image: url\('([^']+)'</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Email">
        <valeur />
      </colonne>
      <colonne mode="valeur" nom="Téléphone">
        <valeur />
      </colonne>
    </colonnes>
  </capture>
</spool>
