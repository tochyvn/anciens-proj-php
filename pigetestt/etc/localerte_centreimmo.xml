            <remplacement>$1/04/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mai ([0-9]+)$</expression>
            <remplacement>$1/05/$2</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) jui(n|\.)? ([0-9]+)$</expression>
            <remplacement>$1/06/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
