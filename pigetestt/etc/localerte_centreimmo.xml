          <filtre>
            <expression>^([0-9]+) sept(embre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/09/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) oct(obre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/10/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) nov(embre|\.)? ([0-9]+)$</expression>
            <remplacement>$1/11/$3</remplacement>
            <option>i</option>
