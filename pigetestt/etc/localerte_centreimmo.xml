          </filtre>
          <filtre>
            <expression>^([0-9]+) f..?v(rier|\.)? ([0-9]+)$</expression>
            <remplacement>$1/02/$3</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([0-9]+) mars ([0-9]+)$</expression>
            <remplacement>$1/03/$2</remplacement>
