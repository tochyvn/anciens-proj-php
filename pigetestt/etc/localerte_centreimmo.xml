            <expression>\.</expression>
            <remplacement />
          </filtre>
          <filtre>
            <expression>,</expression>
            <remplacement>.</remplacement>
          </filtre>
          <filtre>
            <expression>([^0-9\.]+)</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Descriptif">
