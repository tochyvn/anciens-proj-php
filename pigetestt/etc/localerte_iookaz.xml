<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_iookaz">
  <initialisation>
    <url>http://www.iookaz.com/Recherche/Immobilier?Quoi=Quoi&amp;SousSecteur=Location</url>
  </initialisation>
  <navigation>
     <profondeur>300</profondeur>
     <pause>0</pause>
     <timeout>60</timeout>
     <thread>5</thread>
    <rang>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot;&gt;Suivant&lt;\/a&gt;</expression>
         <remplacement>http://www.iookaz.com/Recherche/Immobilier$1</remplacement>
         <limite>1</limite>
         <suivant>0</suivant>
       </a_suivre>
      <a_suivre>
         <expression>&lt;div class=&quot;img&quot;&gt;[^&lt;]+&lt;a href=&quot;([^&quot;]+)&quot;</expression>
         <remplacement>http://www.iookaz.com$1</remplacement>
         <option>s</option>
         <limite>30</limite>
         <suivant>1</suivant>
       </a_suivre>
    </rang>
    <rang>
      <a_capturer>
         <expression>(.+)</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>1</limite>
       </a_capturer>
    </rang>
  </navigation>
  <capture>
     <destination>localerte_iookaz.csv</destination>
     <echappement>\</echappement>
     <separateur>;</separateur>
     <encapsulateur>"</encapsulateur>
     <ligne>CRLF</ligne>
     <enlever_balise>1</enlever_balise>
     <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <option>s</option>
     </parent>
    <enfant>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <limite>1</limite>
	   <option>s</option>
     </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>Iookaz</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>Annonce cr.+e le : ([0-9]{2}\/[0-9]{2}\/[0-9]{4})</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Réf">
        <expression>name=&quot;annonceKey&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>Commune=([^&amp;]+)&amp;amp;SousSecteur=Location&quot;&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="CP">
        <expression>CP=([^&amp;]+)&amp;amp;SousSecteur=Location&quot;&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>Nombre de pi&amp;#232;ces : ([0-9]+)&lt;</expression>
        <remplacement>T$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>&lt;li class=&quot;prix&quot;&gt;([^&lt;]+)&lt;\/li&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>&amp;#8364;</expression>
            <remplacement />
          </filtre>
          <filtre>
            <expression>&amp;#160;</expression>
            <remplacement />
          </filtre>
          <filtre>
            <expression>([^0-9]+)</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Descriptif">
        <expression>&lt;\/div&gt;[^&lt;]+&lt;p&gt;(.+)&lt;\/p&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>name=&quot;annonceTitre&quot; value=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Part/Pro">
        <expression>&lt;li +class=&quot;(pro|part)&quot;&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^pro$</expression>
            <remplacement>PROFESSIONNEL</remplacement>
          </filtre>
          <filtre>
            <expression>^part$</expression>
            <remplacement>PARTICULIER</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;img id=&quot;big_pict&quot; src=&quot;([^&quot;]+)&quot; alt=&quot;</expression>
        <remplacement>http://www.iookaz.com$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Email">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Tél">
        <expression>&lt;li class=&quot;tel&quot;&gt;([^&lt;]+)&lt;\/li&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[^0-9]+</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
    </colonnes>
  </capture>
</spool>
