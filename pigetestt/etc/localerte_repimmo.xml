      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>REPimmo</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>&gt;Annonce du ([0-9]+\/[0-9]+\/[0-9]+)&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^([0-9])\/</expression>
            <remplacement>0$1/</remplacement>
          </filtre>
          <filtre>
            <expression>\/([0-9])\/</expression>
            <remplacement>/0$1/</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression>\/([0-9]+)\/</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>&gt;locations sur (.+) \(([0-9]{5})\)&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="CP">
        <expression>&gt;locations sur (.+) \(([0-9]{5})\)&lt;</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>&gt;Nombre pi..?ces : ([0-9]+) pi..?ces?&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^([0-9]+)$</expression>
            <remplacement>T$1</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>var prix_produit='([^']+)';</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>,</expression>
            <remplacement>.</remplacement>
          </filtre>
          <filtre>
            <expression>[^0-9\.]+</expression>
            <remplacement></remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>&lt;meta name=&quot;description&quot; content=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <option>s</option>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;title&gt;(.+)&lt;\/title&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Part/Pro">
        <expression>(&gt;Agence immobili[^ ]+re|Coordonn[^ ]+es du propri[^ ]+taire)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^&gt;Agence immobili[^ ]+re$</expression>
            <remplacement>PROFESSIONNEL</remplacement>
          </filtre>
          <filtre>
            <expression>^Coordonn[^ ]+es du propri[^ ]+taire$</expression>
            <remplacement>PARTICULIER</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
