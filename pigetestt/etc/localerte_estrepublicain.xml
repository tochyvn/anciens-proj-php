<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_estrepublicain">
  <initialisation>
    <url>http://immo.estrepublicain.fr/fr/immobilier/vente-location/appartement-maison.html?WID_RECH2_R=%2c%2bcType%2520d%2527op%25c3%25a9ration%2f14927154063230018902</url>
  </initialisation>
  <navigation>
     <profondeur>30</profondeur>
     <pause>0</pause>
     <timeout>60</timeout>
     <thread>5</thread>
    <rang>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot; &gt;&lt;img src=&quot;[^&quot;]+&quot; alt=&quot;Page suivante&quot; border=&quot;0&quot; \/&gt;&lt;\/a&gt;</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>1</limite>
         <suivant>0</suivant>
       </a_suivre>
      <a_suivre>
         <expression>&lt;a href=&quot;([^&quot;]+)&quot; title=&quot;[^&quot;]+&quot;&gt;[^&lt;]+&lt;img src=&quot;[^&quot;]+&quot; alt=&quot;voir le d&amp;eacute;tail&quot; \/&gt;&lt;\/a&gt;</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>60</limite>
         <suivant>1</suivant>
       </a_suivre>
    </rang>
    <rang>
      <a_capturer>
         <expression>(.+)</expression>
         <remplacement>$1</remplacement>
         <option>s</option>
         <limite>1</limite>

       </a_capturer>
    </rang>
  </navigation>
  <capture>
     <destination>localerte_estrepublicain.csv</destination>
     <echappement>\</echappement>
     <separateur>;</separateur>
     <encapsulateur>"</encapsulateur>
     <ligne>CRLF</ligne>
     <enlever_balise>1</enlever_balise>
     <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>

	   <option>s</option>
     </parent>
    <enfant>
       <expression>(.+)</expression>
	   <remplacement>$1</remplacement>
	   <limite>1</limite>
	   <option>s</option>
     </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>EstRepublicain</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>Date de publication : ([0-9]{2}\/[0-9]{2}\/[0-9]{4})&lt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Id">
        <expression>([0-9]+)\.html$</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>&lt;strong&gt;Commune:&lt;\/strong&gt;&lt;\/td&gt;&lt;td class=&quot;h&quot; width=&quot;25%&quot;&gt;([^&lt;]+)&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Code postal">
        <expression>&lt;strong&gt;Code postal:&lt;\/strong&gt;&lt;\/td&gt;&lt;td class=&quot;h&quot; width=&quot;25%&quot;&gt;([0-9]{5})&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>&lt;strong&gt;Type de bien:&lt;\/strong&gt;&lt;\/td&gt;&lt;td class=&quot;h&quot; width=&quot;25%&quot;&gt;([^&lt;]+)&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>&lt;strong&gt;Prix:&lt;\/strong&gt;&lt;\/td&gt;&lt;td class=&quot;h&quot; width=&quot;25%&quot;&gt;([^&lt;]+)&amp;nbsp;&amp;euro;&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>	     <option>s</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[^0-9]+</expression>
            <remplacement></remplacement>            <option>i</option>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>&lt;div class=&quot;content&quot; id=&quot;details_annonce&quot;&gt;&lt;p&gt;([^&lt;]+)&lt;\/p&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>	     <option>s</option>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;h1&gt;&lt;strong&gt;([^&lt;]+)&lt;\/strong&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Part/Pro">
        <expression>&lt;a href=&quot;vitrine\.php[^&quot;]+&quot;&gt;Nos annonces&lt;\/a&gt;</expression>
        <remplacement>PROFESSIONNEL</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur></valeur>
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;img src=&quot;([^&quot;]+)&quot; alt=&quot;([^&quot;]+)&quot; id=&quot;imagePrincipale&quot; \/&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Adresse email">
        <valeur></valeur>
      </colonne>
      <colonne mode="expression" nom="Tél">
        <expression>&lt;strong&gt;T..?l : &lt;\/strong&gt;&lt;\/td&gt;&lt;td class=&quot;h&quot; width=&quot;25%&quot;&gt;([^&lt;]+)&lt;\/td&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>	     <option>i</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[^0-9]+</expression>
            <remplacement></remplacement>            <option>i</option>
          </filtre>
        </filtres>
      </colonne>
    </colonnes>
  </capture>
</spool>
