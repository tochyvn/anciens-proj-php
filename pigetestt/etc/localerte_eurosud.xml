<?xml version="1.0" encoding="iso-8859-15"?>
<spool nom="localerte_eurosud">
  <initialisation>
    <url>http://immo.nicematin.com/pqrpa/default/palistsolr?order=recent&amp;louer_localisation=&amp;nb=20&amp;p=:parametre1::parametre2::parametre3:</url>
	<parametres>
	  <parametre>
	    <valeur>0</valeur>
	    <valeur>1</valeur>
	    <valeur>2</valeur>
	    <valeur>3</valeur>
	  </parametre>
	  <parametre>
	    <valeur>0</valeur>
	    <valeur>1</valeur>
	    <valeur>2</valeur>
	    <valeur>3</valeur>
	    <valeur>4</valeur>
	    <valeur>5</valeur>
	    <valeur>6</valeur>
	    <valeur>7</valeur>
	    <valeur>8</valeur>
	    <valeur>9</valeur>
	  </parametre>
	  <parametre>
	    <valeur>0</valeur>
	    <valeur>1</valeur>
	    <valeur>2</valeur>
	    <valeur>3</valeur>
	    <valeur>4</valeur>
	    <valeur>5</valeur>
	    <valeur>6</valeur>
	    <valeur>7</valeur>
	    <valeur>8</valeur>
	    <valeur>9</valeur>
	  </parametre>
	</parametres>
  </initialisation>
  <navigation>
    <profondeur>1400</profondeur>
    <pause>0</pause>
    <timeout>60</timeout>
    <thread>5</thread>
    <utf8>0</utf8>
    <rang>
      <a_suivre>
        <expression>&quot;(http:\/\/immo\.nicematin\.com\/annonce\/[^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <limite>100</limite>
        <suivant>1</suivant>
      </a_suivre>
    </rang>
    <rang>
      <a_capturer>
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <option>s</option>
        <limite>1</limite>
      </a_capturer>
    </rang>
  </navigation>
  <capture>
    <destination>localerte_eurosud.csv</destination>
    <echappement>\</echappement>
    <separateur>;</separateur>
    <encapsulateur>"</encapsulateur>
    <ligne>CRLF</ligne>
    <enlever_balise>1</enlever_balise>
    <enlever_entite>1</enlever_entite>
    <exclusion>
      <fichier>exclusion.csv</fichier>
    </exclusion>
    <parent>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <option>s</option>
    </parent>
    <enfant>
      <expression>(.+)</expression>
      <remplacement>$1</remplacement>
      <limite>1</limite>
      <option>s</option>
    </enfant>
    <colonnes>
      <colonne mode="valeur" nom="Site">
        <valeur>EuroSud</valeur>
      </colonne>
      <colonne mode="valeur" nom="Actif">
        <valeur>1</valeur>
      </colonne>
      <colonne mode="expression" nom="Date">
        <expression>Mis &amp;agrave; jour le ([0-9]{2}\/[0-9]{2}\/[0-9]{4})</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <option>s</option>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="ID">
        <expression>([^\/]+)$</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Ville">
        <expression>        Location (Parking garage|Villa|Maison|[0-9]+ pi..?ces?|4 pi..?ces et plus|Studio - 1 pi..?ce) - (([^\(]+) \(([0-9]+)\))( - ([^&lt;]+))?&lt;\/h1</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
		<option>i</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression> \([0-9]+\)$</expression>
            <remplacement />
          </filtre>
          <filtre>
            <expression> [0-9]+$</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="CP">
        <expression>        Location (Parking garage|Villa|Maison|[0-9]+ pi..?ces?|4 pi..?ces et plus|Studio - 1 pi..?ce) - (([^\(]+) \(([0-9]+)\))( - ([^&lt;]+))?&lt;\/h1</expression>
        <remplacement>$2</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^NICE \(06\)$</expression>
            <remplacement>06000</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^AIX-EN-PROVENCE \(13\)$</expression>
            <remplacement>13090</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^TOULON \(83\)$</expression>
            <remplacement>83000</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^MARSEILLE ([0-9]) \(13\)$</expression>
            <remplacement>1300$1</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^MARSEILLE ([0-9]{2}) \(13\)$</expression>
            <remplacement>130$1</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^MARSEILLE \(13\)$</expression>
            <remplacement>13000</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^AJACCIO \(20\)$</expression>
            <remplacement>20000</remplacement>
            <option>i</option>
          </filtre>
          <filtre>
            <expression>^([^\(]+) \(([0-9]+)\)$</expression>
            <remplacement>$2</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Type">
        <expression>        Location (Parking garage|Villa|Maison|[0-9]+ pi..?ces?|4 pi..?ces et plus|Studio - 1 pi..?ce) - (([^\(]+) \(([0-9]+)\))( - ([^&lt;]+))?&lt;\/h1</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^Studio.*</expression>
            <remplacement>Studio</remplacement>
			<option>i</option>
          </filtre>
          <filtre>
            <expression>^Maison.*</expression>
            <remplacement>Maison</remplacement>
			<option>i</option>
          </filtre>
          <filtre>
            <expression>^Parking garage.*</expression>
            <remplacement />
			<option>i</option>
          </filtre>
          <filtre>
            <expression>^([1-35-9]) pi..?ces?.*</expression>
            <remplacement>T$1</remplacement>
			<option>i</option>
          </filtre>
          <filtre>
            <expression>^4 pi..?ces et plus.*</expression>
            <remplacement />
			<option>i</option>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Loyer">
        <expression>        Location (Parking garage|Villa|Maison|[0-9]+ pi..?ces?|4 pi..?ces et plus|Studio - 1 pi..?ce) - (([^\(]+) \(([0-9]+)\))( - ([^&lt;]+))?&lt;\/h1</expression>
        <remplacement>$5</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>,</expression>
            <remplacement>.</remplacement>
          </filtre>
          <filtre>
            <expression>[^0-9\.]+</expression>
            <remplacement />
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Description">
        <expression>&lt;meta name=&quot;description&quot; content=&quot;([^&quot;]+)&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <option>s</option>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>[ \t]+</expression>
            <remplacement> </remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="expression" nom="Titre">
        <expression>&lt;h1 id=&quot;title-annonce&quot;&gt;([^&lt;]+)&lt;\/h1&gt;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="Part/Pro">
        <expression>(Annonce de professionnel)</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
        <filtres>
          <filtre>
            <expression>^Annonce de professionnel$</expression>
            <remplacement>PROFESSIONNEL</remplacement>
          </filtre>
          <filtre>
            <expression>^PART\.$</expression>
            <remplacement>PARTICULIER</remplacement>
          </filtre>
        </filtres>
      </colonne>
      <colonne mode="valeur" nom="Meublé">
        <valeur />
      </colonne>
      <colonne mode="expression" nom="Photo">
        <expression>&lt;a href=&quot;([^&quot;]+)&quot; class=&quot;multibox-zoom&quot;</expression>
        <remplacement>$1</remplacement>
        <recherche>ENFANT</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="expression" nom="URL">
        <expression>(.+)</expression>
        <remplacement>$1</remplacement>
        <recherche>LOCATION</recherche>
        <limite>1</limite>
      </colonne>
      <colonne mode="valeur" nom="Email">
	    <valeur />
      </colonne>
      <colonne mode="valeur" nom="Tél">
	    <valeur />
      </colonne>
    </colonnes>
  </capture>
</spool>
